public class Robots : Robot
    {
        public Robots()
        {

        }

        public Robots(string robotID, char robotSprite, int startPositionY, int startPositionX)
        {
            base.positionX = startPositionX;
            base.positionY = startPositionY;
            base.RobotID = robotID;
            base.RobotSprite = robotSprite;
        }

        public new int StartPositionY
        {
            get
            {
                return base.StartPositionY;
            }
            set
            {
                base.StartPositionY = value;
            }
        }
        public new int StartPositionX
        {
            get
            {
                return base.StartPositionX;
            }
            set
            {
                base.StartPositionX = value;
            }
        }
        public new int positionX
        {
            get
            {
                return base.positionX;
            }
            set
            {
                base.positionX = value;
            }
        }
        public new int positionY
        {
            get
            {
                return base.positionY;
            }
            set
            {
                base.positionY = value;
            }
        }
        public new string RobotID
        {
            get
            {
                return base.RobotID;
            }
            set
            {
                base.RobotID = value;
            }
        }
        public new char RobotSprite
        {
            get
            {
                return base.RobotSprite;
            }
            set
            {
                base.RobotSprite = value;
            }
        }
    }
